config {
  type: "table"
}

with account as (

    select * 
    from ${constants.SOURCE_PROJECT}.${constants.SOURCE_DATASET}.account
),

fields as (

    select
        id as account_id,
        country,
        -- cast(created_at as {{ dbt.type_timestamp() }}) as created_at,
        created_at,
        name as account_name,
        status
    from account
)

select * 
from fields